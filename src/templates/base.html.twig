<!DOCTYPE html>
<html lang="en">
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>

    {% block stylesheets %}
         {{ encore_entry_link_tags('index_style') }}
         {{ encore_entry_link_tags('nav') }}
         {{ encore_entry_link_tags('content') }}
         {{ encore_entry_link_tags('top_header') }}
         {{ encore_entry_link_tags('header') }}
    {% endblock %}

    {% block javascripts %}
        {{ encore_entry_script_tags('index') }}
    {% endblock %}
</head>
<body>
{% block body %}
    <div id="root"></div>
{% endblock %}
</body>
</html>
